{
    "name": "zarybnicky/tkolymp.cz",
    "description": "TKOlymp.cz",
    "license": "proprietary",
    "type": "project",
    "repositories": [],
    "autoload": {
        "psr-4": { "Olymp\\": "files/" },
        "classmap": [
            "files/Core/",
            "files/Core/database/",
            "files/Core/helpers/",
            "files/Core/types/"
        ],
        "files": ["files/functions.php"]
    },
    "require": {
        "stephank/composer-plugin-nixify": "^1.0",
        "twig/twig": "^3.5",
        "sentry/sdk": "^3.1"
    },
    "require-dev": {
        "sserbin/twig-linter": "@dev"
    },
    "extra": {
        "generate-default-nix": false
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "stephank/composer-plugin-nixify": true
        }
    }
}
