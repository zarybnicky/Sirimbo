{% extends "Layout.twig" %}

{% block content %}
<div class="container">
  <table>
    <tr>
      <th>Pořadí</th>
      <th>Partner</th>
      <th>Partnerka</th>
      <th>Standard</th>
      <th>Latina</th>
      <th>Body</th>
    </tr>
    {% for item in data %}
      <tr>
        <td style="text-align:right;">{{ loop.index }}. </td>
        <td>{{ item.partnerName }}</td>
        <td>{{ item.partnerkaName }}</td>
        <td>{{ item.latina }}</td>
        <td>{{ item.standard }}</td>
        <td>{{ item.hodnoceni }}</td>
      </tr>
      {% endfor %}
  </table>
</div>
{% endblock %}
