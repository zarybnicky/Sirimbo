RewriteEngine On

RewriteCond %{REQUEST_URI} !^/?images/.+ [NC]
RewriteCond %{REQUEST_URI} !^/?style/.+ [NC]
RewriteCond %{REQUEST_URI} !^/?scripts/.+ [NC]
RewriteCond %{REQUEST_URI} !^/?galerie/.+ [NC]
RewriteCond %{REQUEST_URI} !^/?upload/.+ [NC]
RewriteCond %{REQUEST_URI} !^/?devel/.+ [NC]
RewriteCond %{REQUEST_URI} !^/?oklubu/stanovy.pdf$ [NC]
RewriteCond %{REQUEST_URI} !^/?favicon\.ico [NC]
RewriteCond %{REQUEST_URI} !^/?robots\.txt [NC]
RewriteCond $1 !^index\.php.* [NC]
RewriteRule ^(.+)$ /index.php?file=$1 [NC,L,QSA]


php_flag display_startup_errors ofn
php_flag display_errors on
php_flag log_errors on
